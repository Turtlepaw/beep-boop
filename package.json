{
  "name": "beep-boop",
  "packageManager": "yarn@3.4.1",
  "dependencies": {
    "@airdot/activities": "^4.1.6",
    "@airdot/linked-roles": "^0.2.8",
    "@airdot/verifiers": "^2.1.0",
    "@trpc/server": "^10.12.0",
    "axios": "^1.6.0",
    "body-parser": "^1.20.1",
    "canvas": "^2.11.2",
    "captcha-canvas": "^3.2.1",
    "cheerio": "^1.0.0-rc.12",
    "chromium-bidi": "^0.4.5",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "decancer": "^1.6.5",
    "discord-linked-roles": "^0.2.1",
    "discord-trivia": "^2.0.0",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jimp": "^0.16.2",
    "key-file-storage": "^2.2.10",
    "klaw-sync": "^6.0.0",
    "logsnag": "^0.1.6",
    "mitt": "^3.0.0",
    "ms": "^2.1.3",
    "node-fetch": "^2.6.7",
    "pino": "^7.0.0-rc.9",
    "puppeteer": "^19.3.0",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "sqlite3": "^5.1.5",
    "storage-async": "^1.0.1",
    "svg-captcha": "^1.4.0",
    "svg2img": "^1.0.0-beta.2",
    "typeorm": "^0.3.11",
    "unzipper": "^0.10.11",
    "utilsfordiscordjs": "^1.1.8",
    "uuid": "^9.0.0"
  },
  "scripts": {
    "build": "rm -rf ./dist && tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "run": "node ./dist/index.js --preserve-symlinks --require tsconfig-paths/register",
    "lint": "eslint \"src/**\"",
    "dev": "yarn lint && yarn icons && yarn build && yarn run run",
    "dev_": "node ./dev.js",
    "dash": "cd dashboard/ && yarn dev",
    "start": "yarn dev & yarn dash",
    "cache": "yarn run run",
    "fix-canvas": "yarn remove canvas && yarn add canvas",
    "migrate:create": "typeorm migration:create",
    "migrate:run": "typeorm migration:run",
    "pm2": "pm2 start yarn --interpreter bash --watch --ignore-watch=\"dist storage database.sqlite log.json database.sqlite-journal\" -- run run",
    "relink": "yarn link ../skia-canvas && yarn",
    "linkfiles": "git restore package.json && git restore yarn.lock && git pull && yarn relink",
    "pull": "yarn linkfiles && yarn icons && yarn build && pm2 restart BeepBoop",
    "icons": "node generateIcons.js"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2",
    "@types/express": "^4.17.14",
    "@types/ms": "^0.7.31",
    "@types/node-fetch": "^2.6.2",
    "@types/request": "^2.48.8",
    "@types/unzipper": "^0.10.5",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.56.0",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^3.14.2",
    "typeorm": "^0.3.11",
    "typescript": "^4.9.4"
  }
}
